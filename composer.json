{
    "description": "A Symfony bundle that provides a command to easily backup databases. ",
    "license": "MIT",
    "name": "andreasa/backup-database-bundle",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "AndreasA\\BackupDatabaseBundle\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/console": "^4.2 || ^5.0 || ^6.0",
        "symfony/dependency-injection": "^4.2 || ^5.0 || ^6.0",
        "symfony/filesystem": "^4.2 || ^5.0 || ^6.0",
        "symfony/framework-bundle": "^4.2 || ^5.0 || ^6.0",
        "symfony/process": "^4.2 || ^5.0 || ^6.0"
    }
}
